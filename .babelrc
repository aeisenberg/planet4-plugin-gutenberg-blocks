{
    "presets": [
        "@babel/preset-env",
        "preact"
    ],
    "plugins": [
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining",
		[
			"@wordpress/babel-plugin-import-jsx-pragma",
			{
				"scopeVariable": "h",
				"scopeVariableFrag": "Fragment",
				"source": "preact",
				"isDefault": false
			},
		],
        [
            "@babel/plugin-transform-react-jsx", {
                "pragma": "h",
                "runtime": "automatic",
                "importSource": "preact",
                "pragmaFrag": "Fragment"
            }
        ]
    ]
}
